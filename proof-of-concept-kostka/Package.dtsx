          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{7FCD9DFA-E626-4201-8A57-C42D1CC20D72}"
              SQLTask:SqlStatementSource="USE [grades_proof_of_concept]&#xA;GO&#xA;&#xA;INSERT INTO dbo.course_groups (course, course_group, course_group_name, course_type)&#xA;SELECT g.course, -1, 'nieznana grupa kursów',&#xA;CASE&#xA;        WHEN RIGHT(g.course, 1) = 'L' THEN 'laboratorium'&#xA;        WHEN RIGHT(g.course, 1) = 'P' THEN 'laboratorium'&#xA;        WHEN RIGHT(g.course, 1) = 'S' THEN 'laboratorium'&#xA;        WHEN RIGHT(g.course, 1) = 'W' THEN 'laboratorium'&#xA;        ELSE 'inny typ kursu'&#xA;END&#xA;FROM dbo.grades g&#xA;LEFT JOIN dbo.course_groups as cg ON g.course = cg.course&#xA;WHERE (cg.course IS NULL)&#xA;GROUP BY g.course&#xA;GO" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
